            $scope.product = {};

            var token = getUrlParameter('SecureInt');

            //var encrypted = $scope.encrypt(message, $scope.password);
            var decrypted = $scope.decrypt(token, $scope.password);

            $scope.getData = function() {
                productcomplianceform.getData(decrypted.toString(CryptoJS.enc.Utf8)).then(function(res) {
                        $scope.product = res;
                        if (res.product_service) {
                            res.product_service = res.product_service.split(',');
                            angular.forEach(res.product_service,
                                function(value, key) {
                                    $scope.product.product_service[value] = true;
                                });
                        }
                        if (res.identify_issues) {
                            res.identify_issues = res.identify_issues.split(',');
                            angular.forEach(res.identify_issues,
                                function(value, key) {
                                    $scope.product.identify_issues[value] = true;
                                });
                        }
                        if (res.identify_changes) {
                            res.identify_changes = res.identify_changes.split(',');
                            angular.forEach(res.identify_changes,
                                function(value, key) {
                                    $scope.product.identify_changes[value] = true;
                                });
                        }
                        if (res.any_changes) {
                            res.any_changes = res.any_changes.split(',');
                            angular.forEach(res.any_changes,
                                function(value, key) {
                                    $scope.product.any_changes[value] = true;
                                });
                        }
                        if (res.identify_issues_specific) {
                            res.identify_issues_specific = res.identify_issues_specific.split(',');
                            angular.forEach(res.identify_issues_specific,
                                function(value, key) {
                                    $scope.product.identify_issues_specific[value] = true;
                                });
                        }
                        if (res.identify_other_issues) {
                            res.identify_other_issues = res.identify_other_issues.split(',');
                            angular.forEach(res.identify_other_issues,
                                function(value, key) {
                                    $scope.product.product_service[value] = true;
                                });
                        }
                        if (res.include_otherwise) {
                            res.include_otherwise = res.include_otherwise.split(',');
                            angular.forEach(res.include_otherwise,
                                function(value, key) {
                                    $scope.product.include_otherwise[value] = true;
                                });
                        }
                        if (!isNaN(Date.parse(res.release_Date))) {
                            res.release_Date = new Date(res.release_Date);
                        }
                        if (!isNaN(Date.parse(res.date))) {
                            res.date = new Date(res.date);
                        }
                        if (!isNaN(Date.parse(res.prev_completion))) {
                            res.prev_completion = new Date(res.prev_completion);
                        }

                        if (res == '')
                            Component.showInformation('No Records Found');
                        $scope.$apply();
                    },
                    function(error) {
                        Component.handleApiError(error);
                    });

            }

            $scope.getData();

            function getUrlParameter(name) {
                name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
                var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
                var results = regex.exec(location.search);
                return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
            };

            function downloadPdf(base64String, fileName) {
                if (window.navigator && window.navigator.msSaveOrOpenBlob) {
                    // download PDF in IE
                    var byteChar = atob(base64String);
                    var byteArray = new Array(byteChar.length);
                    for (let i = 0; i < byteChar.length; i++) {
                        byteArray[i] = byteChar.charCodeAt(i);
                    }
                    var uIntArray = new Uint8Array(byteArray);
                    var blob = new Blob([uIntArray], { type: 'application/pdf' });
                    window.navigator.msSaveOrOpenBlob(blob, `${fileName}.pdf`);
                } else {
                    // Download PDF in Chrome etc.
                    var source = `data:application/pdf;base64,${base64String}`;
                    var link = document.createElement("a");
                    link.href = source;
                    link.download = `${fileName}.pdf`;
                    link.click();
                }
            }

            $scope.onClickDownloadPdf = function () {
                downloadPdf($scope.product.file_Upload, "Download");
            }
        });
</script>
