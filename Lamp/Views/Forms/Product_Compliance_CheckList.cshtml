@section scripts{
<script src="~/js/api/productcomplianceform.js"></script>
<script type="text/javascript">
    //var myApp = angular.module('myApp', [])

    myApp.controller(
      "myCtrl",
      function ($scope, $filter, $http, $location, $uibModal) {
        $scope.product = {};
        $scope.getUsers = function () {
          productcomplianceform.getUsers().then(function (res) {
            if (res) {
              $scope.names = res;
            }
            //$scope.submission();
          });
        };
        $scope.product.include_otherwise = 0;
        $scope.getUsers();
        $scope.Save = function () {
          $scope.product.product_serviceconcat = "";
          $scope.product.identify_issuesconcat = "";
          $scope.product.identify_changesconcat = "";
          $scope.product.any_changesconcat = "";
          $scope.product.identify_issues_specificconcat = "";
          $scope.product.identify_other_issuesconcat = "";
          $scope.product.include_otherwiseconcat = "";
          angular.forEach($scope.product.product_service, function (value, key) {
            $scope.product.product_serviceconcat += key + ",";
          });
          $scope.product.product_service =
            $scope.product.product_serviceconcat.slice(0, -1);
          angular.forEach($scope.product.identify_issues, function (value, key) {
            $scope.product.identify_issuesconcat += key + ",";
          });
          $scope.product.identify_issues =
            $scope.product.identify_issuesconcat.slice(0, -1);
          angular.forEach($scope.product.identify_changes, function (value, key) {
            $scope.product.identify_changesconcat += key + ",";
          });
          $scope.product.identify_changes =
            $scope.product.identify_changesconcat.slice(0, -1);
          angular.forEach($scope.product.any_changes, function (value, key) {
            $scope.product.any_changesconcat += key + ",";
          });
          $scope.product.any_changes = $scope.product.any_changesconcat.slice(
            0,
            -1
          );
          angular.forEach(
            $scope.product.identify_issues_specific,
            function (value, key) {
              $scope.product.identify_issues_specificconcat += key + ",";
            }
          );
          $scope.product.identify_issues_specific =
            $scope.product.identify_issues_specificconcat.slice(0, -1);
          angular.forEach(
            $scope.product.identify_other_issues,
            function (value, key) {
              $scope.product.identify_other_issuesconcat += key + ",";
            }
          );
          $scope.product.identify_other_issues =
            $scope.product.identify_other_issuesconcat.slice(0, -1);
          angular.forEach(
            $scope.product.include_otherwise,
            function (value, key) {
              $scope.product.include_otherwiseconcat += key + ",";
            }
          );
          $scope.product.include_otherwise =
            $scope.product.include_otherwiseconcat.slice(0, -1);

          productcomplianceform.Save($scope.product).then(function (res) {
            if (res) {
              var dataToSend = "";
              var dataToSend = {
                Id: res,
                Encrypted: $scope.encrypt(res.toString(), $scope.password),
              };
              productcomplianceform
                .UpdateEncyptedKey(dataToSend)
                .then(function (response) {
                  console.log("Mail Sent" + response);
                  Component.showSuccess("Saved Successfully");
                  $scope.submission();
                });
            }
            //$scope.submission();
          });
        };

        $scope.submission = function () {
          $scope.open = $uibModal.open({
            templateUrl: "Submission.html",
            backdrop: false,
          });
        };

        document
          .getElementById("file_Upload")
          .addEventListener("change", handleFileSelect, false);

        function handleFileSelect(evt) {
          var f = evt.target.files[0]; // FileList object
          var reader = new FileReader();
          // Closure to capture the file information.
          reader.onload = (function (theFile) {
            return function (e) {
              var binaryData = e.target.result;
              //Converting Binary Data to base 64
              var base64String = window.btoa(binaryData);
              //showing file converted to base64
              $scope.product.file_Upload = base64String;
            };
          })(f);
          // Read in the image file as a data URL.
          reader.readAsBinaryString(f);
        }
      }
    );
</script>
