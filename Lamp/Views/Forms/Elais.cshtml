        $scope.next = function (form, step) {
            $scope.formValid = true;
            if (form != null && !form) {
                $scope.formValid = false;
            }

            switch (step) {
                case 1:
                    if (!form) {
                        Component.showError("Please Fill The Required Fields");
                    }
                    break;
                case 2:
                    if (!form) {
                        $('a[href = "#step-3"]').click();
                        Component.showError("Please Fill The Required Fields");
                    }
                    break;
                case 3:
                    if (!form) {
                        $('a[href = "#step-4"]').click();
                        Component.showError("Please Fill The Required Fields");
                    } else if (form == true) {

                        if ($scope.elais.enduserPrimaryBussinessFunc == undefined) {
                            $('a[href = "#step-4"]').click();
                        }
                        else {
                            var a = false;
                            angular.forEach($scope.elais.enduserPrimaryBussinessFunc, function (value, key) {

                                if (value == true) { a = true; return false; }
                            });

                            if (a == false) { $('a[href = "#step-4"]').click(); }
                        }
                    }
                    break;

                case 4:
                    if (!form) {
                        $('a[href = "#step-5"]').click();
                        Component.showError("Please Fill The Required Fields");
                    } else if (form == true) {

                        if ($scope.elais.enduserproductsandservices == undefined) {
                            $('a[href = "#step-5"]').click();
                        }
                        else {
                            var a = false;
                            angular.forEach($scope.elais.enduserproductsandservices, function (value, key) {

                                if (value == true) { a = true; return false; }
                            });

                            if (a == false) { $('a[href = "#step-5"]').click(); }
                        }
                    }
                    break;
            }
        }

