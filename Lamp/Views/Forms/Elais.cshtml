@{
    ViewData["Title"] = "Export License Application Information Sheet ('ELAIS')";
}


<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link href="~/assets/EliasForm/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.12.1/jquery-ui.css" />
    <link href="~/assets/EliasForm/css/mdb.min.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/assets/stencil_v1/style.css">
    <link href="~/assets/EliasForm/css/main.css" rel="stylesheet" />

    <style>
        [data-tip] {
            position: relative;
        }

            [data-tip]:before {
                content: '';
                /* hides the tooltip when not hovered */
                display: none;
                content: '';
                border-left: 5px solid transparent;
                border-right: 5px solid transparent;
                border-bottom: 5px solid #1a1a1a;
                position: absolute;
                top: 30px;
                left: 35px;
                z-index: 8;
                font-size: 0;
                line-height: 0;
                width: 0;
                height: 0;
            }

            [data-tip]:after {
                display: none;
                content: attr(data-tip);
                position: absolute;
                top: 35px;
                left: 0px;
                padding: 5px 8px;
                background: #1a1a1a;
                color: #fff;
                z-index: 9;
                font-size: 0.75em;
                height: 30px;
                line-height: 18px;
                -webkit-border-radius: 3px;
                -moz-border-radius: 3px;
                border-radius: 3px;
                white-space: nowrap;
                word-wrap: normal;
            }

            [data-tip]:hover:before,
            [data-tip]:hover:after {
                display: block;
            }
    </style>
</head>
<body>
    <header>
        <div class="container">
            <div class="branding">
                <img src="~/assets/img/juniper-logo-white.svg" width="166" height="54" border="0" alt="Juniper Networks" title="Juniper Networks" />
            </div>
        </div>
    </header>
    <div id="main-container">
        <h2 class="text-center font-bold pt-4 pb-5 mb-5">
            <strong>Export License Application Information Sheet ("ELAIS")</strong>
        </h2>
        <!-- Stepper -->
        <div id="steps-form-2-main">
            <div class="steps-form-2">
                <div class="steps-row-2 setup-panel-2 d-flex justify-content-between">
                    <div class="steps-step-2" style="display: none">
                        <a href="#step-1" type="button" class="btn btn-amber btn-circle-2 waves-effect ml-0" data-toggle="tooltip" data-placement="top" title="Advisory"><i class="fa fa-folder-open-o" aria-hidden="true"></i></a>
                    </div>
                    <div class="steps-step-2">
                        <a href="#step-2" type="button" class="btn btn-blue-grey btn-circle-2 waves-effect" data-toggle="tooltip" data-placement="top" title="Submitter Information"><i class="fa fa-pencil" aria-hidden="true"></i></a>
                        @*<p>Submitter Information</p>*@
                    </div>
                    <div class="steps-step-2">
                        <a href="#step-3" type="button" class="btn btn-blue-grey btn-circle-2 waves-effect" data-toggle="tooltip" data-placement="top" title="Transaction Information"><i class="fa fa-pencil" aria-hidden="true"></i></a>
                        @*<p>Transaction Information</p>*@
                    </div>
                    <div class="steps-step-2">
                        <a href="#step-4" type="button" class="btn btn-blue-grey btn-circle-2 waves-effect mr-0" data-toggle="tooltip" data-placement="top" title="Parties Involved"><i class="fa fa-pencil" aria-hidden="true"></i></a>
                        @*<p>Parties Involved</p>*@
                    </div>
                    <div class="steps-step-2">
                        <a href="#step-5" type="button" class="btn btn-blue-grey btn-circle-2 waves-effect mr-0" style="margin-left: 1px;" data-toggle="tooltip" data-placement="top" title="End Use of Juniper Products and Services"><i class="fa fa-pencil" aria-hidden="true"></i></a>
                        @*<p>End Use of Juniper Products and Services</p>*@
                    </div>
                    <!-- <div class="steps-step-2">
                        <a href="#step-6" type="button" class="btn btn-blue-grey btn-circle-2 waves-effect mr-0" data-toggle="tooltip" data-placement="top" title="Installation Sites"><i class="fa fa-pencil" aria-hidden="true"></i></a>
                    </div> -->
                    <div class="steps-step-2" style="display:none;">
                        <a href="#step-7" type="button" class="btn btn-blue-grey btn-circle-2 waves-effect mr-0" data-toggle="tooltip" data-placement="top" title="Certify"><i class="fa fa-check" aria-hidden="true"></i></a>
                    </div>
                </div>
            </div>
        </div>
        <!-- First Step -->
        <div class="row setup-content-2" id="step-1" style="margin-top: -50px;">
            <div class="col-md-12">
                <section>
                    <h3>Purpose:</h3>
                    <p>Juniper relies on the information that you provide in this form to determine what export licenses, if any, are required for your transaction. If Juniper concludes that an export license is required, then it will use the information provided to apply for any required license(s).  </p>
                    <h3>Who needs to complete this form?</h3>
                    <p>An authorized representative of a Juniper channel partner must complete this form. The representative must be familiar with the applicable transaction either personally or through relevant business records. </p>
                    <h3 style="margin-bottom: 20px;">Advisory:</h3>
                    <h4>It is critical that the information provided below be current, accurate and complete, <span id="kyc" style="text-transform:none"><u>Remember, it’s up to you as a Juniper channel partner to “know your customer.”</u></span></h4>
                    <h4>This form contains information auto-filled based on data in juniper networks' records. <span id="kyc" style="text-transform:none"><u>It is your responsibility to verify and, if necessary, correct any inaccurate or incomplete information that has been auto-filled. By submitting this form you, on behalf of the Juniper channel partner whom you are representing, are certifying as to the accuracy of ALL information in the form, including any auto-filled information that you do not modify.</u></span></h4>
                    <h3>Consequences of Providing Obsolete, Inaccurate or Incomplete Information:</h3>
                    <p>Obsolete, inaccurate or incomplete information may lead to: </p>
                    <ul>
                        <li>Shipping delays;</li>
                        <li>Additional disclosures to export authorities;</li>
                        <li>Inability for Juniper to provide support services;</li>
                        <li>Increased restrictions on future transactions;</li>
                        <li>Termination of your channel partner agreement; and</li>
                        <li>Civil and criminal liability.</li>
                    </ul>
                    <h3>Questions and Assistance:</h3>
                    <p>If you have any questions regarding the questions in this form or if you have any concerns that information you rely on in answering the questions might be inaccurate or incomplete, please contact <a href="#">tradecompliance@juniper.net</a> .</p>
                    <p>In addition, please contact <a href="#">tradecompliance@juniper.net</a> if after completing this form you learn: </p>
                    <ul>
                        <li>that any of the information furnished was inaccurate or incomplete</li>
                        <li>that the transaction itself or the parties or the intended end use has changed; or  </li>
                        <li>that new information obtained raises concerns about the accuracy of the information furnished.</li>
                    </ul>
                </section>
                <button class="btn btn-mdb-color btn-rounded nextBtn-2 float-right" type="button" onclick="show25()">Next</button>
            </div>
        </div>

        <!-- Second Step -->
        <form name="Form1" novalidate>
            <div class="row setup-content-2" id="step-2">
                <div class="col-md-12">
                    <section>
                        <h1>Submitter Information</h1>
                        <span id="mandatory">*Indicates Mandatory Fields </span>
                        <p>
                            <span class="text-danger">
                                NOTE:
                            </span>
                            <li>Providing inaccurate or incomplete information on this form may lead to shipping delays.</li>
                            <li>Provide all information in <b>English</b>. Submissions containing abbreviations, acronyms or a P.O. Box number will be considered invalid.</li>
                        </p>
                        <div id="basic-info">
                            <label>
                                <span style="display: inline-block;">Submitted by*</span>
                                <input type="text" name="fname" placeholder="First Name" maxlength="50" ng-model="elais.firstName" required><input style="margin-left: 20px" maxlength="50" placeholder="Last Name" type="text" name="lname" ng-model="elais.lastName" required>&nbsp;&nbsp;<span style="color:red" ng-show="!formValid && Form1.fname.$invalid ||!formValid && Form1.lname.$invalid"><span ng-show="Form1.fname.$error.required||Form1.lname.$error.required">First Name and Last Name is required.</span></span>
                            </label>
                            <label>
                                <span style="display: inline-block;">Job title*</span>
                                <input type="text" name="jobtitle" placeholder="Job Title" maxlength="50" ng-model="elais.jobTitle" required>
                                <span style="color:red" ng-show="!formValid && Form1.jobtitle.$invalid "><span ng-show="Form1.jobtitle.$error.required">Job title is required.</span></span>
                            </label>
                            <label>
                                <span style="display: inline-block;">Legal Name of Channel Partner*</span>
                                <input type="text" name="channelpartner" placeholder="Full Legal Name of Juniper Channel Partner" maxlength="50" ng-model="elais.channelPartner" required>
                                <span style="color:red" ng-show="!formValid && Form1.channelpartner.$invalid"><span ng-show="Form1.channelpartner.$error.required">Channel Partner is required.</span></span>
                            </label>
                            <label>
                                <span style="display: inline-block;">Email Address*</span>
                                <input type="text" name="email" placeholder="Email" maxlength="30" ng-model="elais.emailAddress" required>
                                <span style="color:red" ng-show="!formValid && Form1.email.$invalid "><span ng-show="Form1.email.$error.required">Email is required.</span></span>
                            </label>
                            <label>
                                <span style="display: inline-block;">Juniper VAR ID</span>
                                <input type="text" name="user-id" placeholder="Juniper VAR ID" ng-model="elais.JuniperVar">
                            </label>
                            @*<div id="foot">
                                <p>
                                    NOTE:
                                    <li>Providing inaccurate or incomplete information on this form may lead to shipping delays.</li>
                                    <li>Provide all information in <b>English</b>. Submissions containing abbreviations, acronyms or a P.O. Box number will be considered invalid.</li>
                                </p>
                            </div>*@
                        </div>
                    </section>
                    <button class="btn btn-mdb-color btn-rounded prevBtn-2 float-left" type="button" onclick="show26()">Previous</button>
                    <button class="btn btn-mdb-color btn-rounded nextBtn-2 float-right" ng-click="next(Form1.$valid,1)" type="button">Next</button>
                </div>
            </div>
        </form>

        <!-- Third Step -->
        <form name="Form2" novalidate>
            <div class="row setup-content-2" id="step-3">
                <div class="col-md-12">
                    <section>
                        <h1>Transaction Information</h1>
                        <span id="mandatory">*Indicates Mandatory Fields </span>
                        <p>
                            <span class="text-danger">
                                NOTE:
                            </span>
                            <li>Please take the transaction reference numbers from the email request sent to you by Trade Compliance </li>
                        </p>
                        <div id="basic-info">
                            <label>
                                <span data-toggle="tooltip" title="Please take the ‘Quote ID’ from the email request sent by Trade Compliance.">Quote #*</span><span data-tip="Please take the ‘Quote ID’ from the email request sent by Trade Compliance."><input type="text" name="qnum" placeholder="Quote #" ng-model="elais.quoteNumber" required></span>
                                <span style="color:red" ng-show="!formValid && Form2.qnum.$invalid "><span ng-show="Form2.qnum.$error.required">Quote Number is required.</span></span>
                            </label>
                            @*<label>
                                <span>Quote Date*</span><input type="date" name="qdate" id="datepicker" placeholder="Quote Date" ng-model="elais.quoteDate" required>
                                <span style="color:red" ng-show="!formValid && Form2.qdate.$invalid "><span ng-show="Form2.qdate.$error.required">Quote Date is required.</span></span>
                            </label>*@
                            <label>
                                <span data-toggle="tooltip" title="Please take the ‘Purchase Order’ (if available) from the email request sent by Trade Compliance, or leave it blank(if not available).">Purchase Order #</span><span data-tip="Please take the ‘Purchase Order’ (if available) from the email request sent by Trade Compliance, or leave it blank(if not available)."><input type="text" name="opp" placeholder="Purchase Order" ng-model="elais.purchaseOrder"></span>
                            </label>
                            <label>
                                <span data-toggle="tooltip" title="Please take the ‘Sales Order’  (if available) from the email request sent by Trade Compliance, or leave it blank(if not available).">Sales Order #</span><span data-tip="Please take the ‘Sales Order’  (if available) from the email request sent by Trade Compliance, or leave it blank(if not available)."><input type="text" name="sales" placeholder="Sales Order" ng-model="elais.salesOrder"></span>
                            </label>
                            <!-- <label>
                                <span>NSP #</span><input type="text" name="nsp" placeholder="NSP">
                            </label> -->
                        </div>
                        @*<div id="foot">
                            <p>NOTE: Providing inaccurate or incomplete information on this form may lead to shipping delays.</p>
                        </div>*@
                    </section>
                    <button class="btn btn-mdb-color btn-rounded prevBtn-2 float-left" type="button">Previous</button>
                    <button class="btn btn-mdb-color btn-rounded nextBtn-2 float-right" ng-click="next(Form2.$valid,2)" type="button">Next</button>
                </div>
            </div>
        </form>

        <!-- Fourth Step -->
        <div class="row setup-content-2" id="step-4">
            <form name="Form3" novalidate>
                <div class="col-md-12">
                    <div class="row">
                        <section style="margin-bottom: 0px; padding-bottom: 10px;">
                            <h1 style="margin-top: 0px"><!-- <span data-toggle="tooltip" title="A Top Level Purchaser is the party that places purchase order directly on the Juniper buy-sell entity. A DVAR purchasing for resale to an end user is the 'Top Level Purchaser'. A distributor purchasing stock-able items for stock is also a 'Top Level Purchaser'. A direct Juniper end user customer that is purchasing directly from Juniper in a transaction is also a 'Top Level Purchaser' in that transaction."> -->Parties Involved<!-- </span> --></h1>
                            <span id="mandatory">
                                *Indicates Mandatory Fields
                            </span>
                            <p>
                                <span class="text-danger">
                                    NOTE:
                                </span>
                                <li>Providing inaccurate or incomplete information on this form may lead to shipping delays.</li>
                                <li>Provide all information in <b>English</b>. Submissions containing abbreviations, acronyms or a P.O. Box number will be considered invalid.</li>
                            </p>
                        </section>
                    </div>
                    <div class="panel panel-default">
                        <div class="panel-heading"><h3><span data-toggle="tooltip" title="The party who is purchasing the product directly from Juniper or Juniper's Authorized Distributor">Distributor or Reseller</span></h3></div>
                        <div class="panel-body">
                            <div id="basic-info">
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>
                                            <span>Company Name* </span>
                                            <input type="text" name="purchasername" placeholder="Company Name" ng-model="elais.purchaserName" required>
                                        </label>
                                        <span ng-show="!formValid && Form3.purchasername.$invalid "></span>
                                        <span style="color:red;text-align:center" ng-show="!formValid && Form3.purchasername.$invalid "><span ng-show="Form3.purchasername.$error.required">Company Name is required.</span></span>
                                        <label>
                                            <span>Street*</span>
                                            <input type="text" name="purchaserstreet" placeholder="Street" ng-model="elais.purchaserStreet" required>
                                        </label>
                                        <span ng-show="!formValid && Form3.purchaserstreet.$invalid "></span>
                                        <span style="color:red;text-align:center" ng-show="!formValid && Form3.purchaserstreet.$invalid "><span ng-show="Form3.purchaserstreet.$error.required">Street is required.</span></span>
                                        <label>
                                            <span>City*</span>
                                            <input type="text" name="purchasercity" placeholder="City" ng-model="elais.purchaserCity" required>
                                        </label>
                                        <span ng-show="!formValid && Form3.purchasercity.$invalid "></span>
                                        <span style="color:red;text-align:center" ng-show="!formValid && Form3.purchasercity.$invalid "><span ng-show="Form3.purchasercity.$error.required">City is required.</span></span>
                                    </div>
                                    <div class="col-md-6">
                                        <label>
                                            <span>State</span>
                                            <input type="text" name="purchaser-state" placeholder="State" ng-model="elais.purchaserState">
                                        </label>
                                        <label>
                                            <span>Postal Code</span>
                                            <input type="text" name="purchaser-postal" placeholder="Postal Code" ng-model="elais.purchaserPostalCode">
                                        </label>
                                        <label>
                                            <span>Country*</span>
                                            <select name="pruchasercountry" ng-model="elais.purchaserCountry" required>
                                                <option value="">Select Country</option>
                                                <option ng-repeat="data in Countrylist | orderBy: 'name'" value="{{data.id}}">{{data.name}}</option>
                                            </select>
                                        </label>
                                        <span ng-show="!formValid && Form3.pruchasercountry.$invalid "></span>
                                        <span style="color:red;text-align:center" ng-show="!formValid && Form3.pruchasercountry.$invalid "><span ng-show="Form3.pruchasercountry.$error.required">Country is required.</span></span>
                                        <!--  <label>
                                            <span>Juniper 8-digit Partner Number:</span>
                                            <input id="partner-number-input" type="text" name="partner-number">
                                        </label>
                                        <label>
                                            <input type="radio" name="number-check" onclick="show15()">
                                            <span>No Partner Number</span>
                                        </label> -->
                                    </div>
                                </div>

                                <div>
                                    <span style="width:auto;margin-left: 10px;margin-right: 97px;">Is this party a distributor or reseller*</span>
                                    @*<form style="display: initial; margin-left:60px; margin-right:189px;">
                                        *@
                                        <input type="radio" name="dstrbrese" value="Distributor" ng-model="elais.purchaserreseller" required> Distributor
                                        <input type="radio" name="dstrbrese" value="Reseller" ng-model="elais.purchaserreseller" required> Reseller
                                        @*
                                    </form>*@
                                    <span style="color:red" ng-if="!formValid && Form3.dstrbrese.$invalid ">Please Select One</span>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3>
                                <span data-toggle="tooltip" title="'End User' is the party that will be placing the product in use for its own benefit and not for resale or other further distribution.
                The End User is not a system integrator purchasing product for a client for whom the SI will make the product available.
                The End User is not a purchasing agent for the ultimate end user of the product, nor is the End User a mere systems administrator.
                A managed services provider purchasing equipment, software or services for the sole benefit of a pre-determined customer is not an end user; the predetermined customer is.
                But a managed services provider purchasing equipment, software or services for the the benefit of the general public is treated as the end user in the transaction in which Juniper is involved..)
                BEWARE: Don't confuse 'End User' for trade compliance purposes with 'End Customer' as shown in some CRM/SAP data.  End Customer is not always the true End User!!">End User</span>
                            </h3>
                        </div>
                        <div class="panel-body">
                            <div id="basic-info">
                                <div class="row">
                                    <div class="col-md-6">
                                        <label>
                                            <span>Company Name* </span>
                                            <input type="text" name="enduserentityname" placeholder="Company Name" ng-model="elais.enduserCompanyName" required>
                                        </label>
                                        <span ng-show="!formValid && Form3.enduserentityname.$invalid "></span>
                                        <span style="color:red;text-align:center" ng-show="!formValid && Form3.enduserentityname.$invalid "><span ng-show="Form3.enduserentityname.$error.required">End Company Name is required.</span></span>
                                        <label>
                                            <span>Street*</span>
                                            <input type="text" name="enduserstreet" placeholder="Street" ng-model="elais.enduserStreet" required>
                                        </label>
                                        <span ng-show="!formValid && Form3.enduserstreet.$invalid "></span>
                                        <span style="color:red;text-align:center" ng-show="!formValid && Form3.enduserstreet.$invalid "><span ng-show="Form3.enduserstreet.$error.required">End User Street is required.</span></span>
                                        <label>
                                            <span>City*</span>
                                            <input type="text" name="endusercity" placeholder="City" ng-model="elais.enduserCity" required>
                                        </label>
                                        <span ng-show="!formValid && Form3.endusercity.$invalid "></span>
                                        <span style="color:red;text-align:center" ng-show="!formValid && Form3.endusercity.$invalid "><span ng-show="Form3.endusercity.$error.required">End User City is required.</span></span>
                                        <label>
                                            <span>State</span>
                                            <input type="text" name="end-user-state" placeholder="State" ng-model="elais.enduserState">
                                        </label>
                                    </div>
                                    <div class="col-md-6">
                                        <label>
                                            <span>Postal Code</span>
                                            <input type="text" name="end-user-postal" placeholder="Postal Code" ng-model="elais.enduserPostalcode">
                                        </label>
                                        <label>
                                            <span>Country*</span>
                                            <select name="endusercountry" ng-model="elais.enduserCountry" required>
                                                <option value="">Select Country</option>
                                                <option ng-repeat="data in Countrylist | orderBy: 'name'" value="{{data.id}}">{{data.name}}</option>
                                            </select>
                                        </label>
                                        <span ng-show="!formValid && Form3.endusercountry.$invalid "></span>
                                        <span style="color:red;text-align:center" ng-show="!formValid && Form3.endusercountry.$invalid "><span ng-show="Form3.endusercountry.$error.required">End User Country is required.</span></span>
                                        <label>
                                            <span>End User Contact  First and Last Name*</span>
                                            <input type="text" name="endusername" placeholder="End User Contact  First and Last Name" ng-model="elais.enduserContactName" required>
                                        </label>
                                        <span ng-show="!formValid && Form3.endusername.$invalid "></span>
                                        <span style="color:red;text-align:center" ng-show="!formValid && Form3.endusername.$invalid "><span ng-show="Form3.endusername.$error.required">End User Contact Name is required.</span></span>
                                        <label>
                                            <span>End User Contact  Email Address*</span>
                                            <input type="text" name="enduseremail" placeholder="End User Contact  Email Address" ng-model="elais.enduserEmail" required>
                                        </label>
                                        <span ng-show="!formValid && Form3.enduseremail.$invalid "></span>
                                        <span style="color:red;text-align:center" ng-show="!formValid && Form3.enduseremail.$invalid "><span ng-show="Form3.enduseremail.$error.required">End User Email is required.</span></span>
                                    </div>
                                </div>

                                <div>
                                    <span style="width:auto;margin-left: 10px;margin-right: 97px;">Is there a URL for the End User’s Official Website ? e.g. https://www.enduser.com (no email addresses)*</span>
                                    @*<form style="display: initial; margin-left:60px; margin-right:189px;">
                                        *@
                                        <input type="radio" name="webcheck" value="yes" ng-model="elais.enduserofficial" ng-click="elais.enduserOfficialText=''" required> Yes
                                        <input type="radio" name="webcheck" value="no" ng-model="elais.enduserofficial" ng-click="elais.enduserOfficialText=''" required> No
                                        <span style="color:red" ng-if="!formValid && Form3.webcheck.$invalid ">&nbsp;&nbsp; Please Select One.</span>
                                        @*
                                    </form>*@
                                    <input id="end-user-url" ng-if="elais.enduserofficial == 'yes'" style="margin-left: 189px;" ng-model="elais.enduserOfficialText" placeholder="End User URL" type="text" name="urlname" required>
                                    <span style="color:red;margin-right: 42px;margin-top: 2px;" class="pull-right" ng-if="!formValid && Form3.urlname.$invalid "><span ng-if="Form3.urlname.$error.required">End User URL is required.</span></span>
                                    <input id="end-user-url" ng-if="elais.enduserofficial == 'no'" style="margin-left: 189px;" ng-model="elais.enduserOfficialText" placeholder="Reason for No Website" type="text" name="urlname1" required>
                                    <span style="color:red;margin-right: 42px;margin-top: 2px;" class="pull-right" ng-if="!formValid && Form3.urlname1.$invalid "><span ng-if="Form3.urlname1.$error.required">Reason is required.</span></span>
                                </div>
                            </div>
